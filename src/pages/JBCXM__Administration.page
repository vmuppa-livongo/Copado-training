            function pushJbaraNameSpace(item, nameSpacePrefix) {
                for (var key in item) {
                    if (key != 'Id' && key != 'Name') {
                        item[nameSpacePrefix + key] = item[key];
                        delete item[key];
                    }
                }
                return item;
            }
    
