    <fields>
        <fullName>Gross_Program_TCV__c</fullName>
        <externalId>false</externalId>
        <formula>CASE(Program_Family__c,
        &apos;Diabetes&apos;, Product_Participants__c * 0.25 * UnitPrice * Quantity + Currently_Enrolled_Members__c * Incremental_Sales_Price__c * Quantity,
        &apos;Hypertension&apos;, Product_Participants__c * 0.10 * UnitPrice * Quantity + Currently_Enrolled_Members__c * Incremental_Sales_Price__c * Quantity,
        &apos;Weight Management&apos;, IF(UnitPrice == 1.00, 
                                (500 * Product_Participants__c * 0.10 + Product_Participants__c * 0.10 * Year_2_Price__c * MAX(Quantity - 12, 0)) + Currently_Enrolled_Members__c * Incremental_Sales_Price__c * Quantity, 
                                (Product_Participants__c * 0.10 * UnitPrice * 12 + Product_Participants__c * 0.10 * Year_2_Price__c * MAX(Quantity - 12, 0)) + Currently_Enrolled_Members__c * Incremental_Sales_Price__c * Quantity), 
        &apos;Prediabetes&apos;, IF(UnitPrice == 1.00, 
                            (500 * Product_Participants__c * 0.10 + Product_Participants__c * 0.10 * Year_2_Price__c * MAX(Quantity - 12, 0)) + Currently_Enrolled_Members__c * Incremental_Sales_Price__c * Quantity, 
                            (Product_Participants__c * 0.10 * UnitPrice * 12 + Product_Participants__c * 0.10 * Year_2_Price__c * MAX(Quantity - 12, 0)) + Currently_Enrolled_Members__c * Incremental_Sales_Price__c * Quantity),
        Product_Participants__c * UnitPrice * Quantity + Currently_Enrolled_Members__c * Incremental_Sales_Price__c * Quantity)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Gross Program EVA</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Currency</type>
