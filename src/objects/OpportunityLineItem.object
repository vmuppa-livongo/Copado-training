    <validationRules>
        <fullName>Cannot_Change_Price_without_Structure</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
ISCHANGED( UnitPrice ),
ISBLANK( Pricing_Structure__c )
)</errorConditionFormula>
        <errorMessage>Please add a Pricing Structure to update the sales price</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cannot_Update_Incre_Price_Enrolled_Parti</fullName>
        <active>true</active>
        <description>Cannot update incremental sales price and existing enrolled program participants if the the program is not a WP program</description>
        <errorConditionFormula>AND(
OR(ISCHANGED(Currently_Enrolled_Members__c),
ISCHANGED(Incremental_Sales_Price__c)),
NOT(Product2.Whole_Person_Solution__c))</errorConditionFormula>
        <errorMessage>Existing Enrolled Program Participants and Incremental Sales Price cannot be edited for non Whole Person Solutions.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Initial_Price_must_be_0</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
 ISNEW(),
 UnitPrice != 0
)</errorConditionFormula>
        <errorMessage>Initial Sales Price must be 0. Please use the lookup &quot;Pricing Structure&quot; to alter this Sales price. Prices can be updated after creation.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PMPM_only_for_BH_Product</fullName>
        <active>true</active>
        <description>PMPM rate can only be edited for BH 2.0 product</description>
        <errorConditionFormula>AND(ISCHANGED(PMPM__c), 
NOT(ISPICKVAL(Product2.Family, &apos;Behavioral Health&apos;)))</errorConditionFormula>
        <errorDisplayField>PMPM__c</errorDisplayField>
        <errorMessage>PMPM should only be entered for Behavioral Health products</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Pricing_Must_be_in_Range</fullName>
        <active>false</active>
        <errorConditionFormula>OR(
 AND(
  !ISNEW(),
  !ISBLANK( PricebookEntry.Price_Floor__c ),
  UnitPrice &lt;= PricebookEntry.Price_Floor__c,
  $User.ProfileId != &quot;00e46000001QIv0&quot;
 ),
 AND(
  !ISNEW(),
  !ISBLANK( PricebookEntry.Price_Ceiling__c ),
  UnitPrice &gt;= PricebookEntry.Price_Floor__c,
  $User.ProfileId != &quot;00e46000001QIv0&quot;
 )
)</errorConditionFormula>
        <errorDisplayField>UnitPrice</errorDisplayField>
        <errorMessage>You&apos;re attempting to update the Sales Price outside of the defined range for this program. Please contact your Sales Admin to request an exception.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>WP_Programs_Pricing_Must_be_in_Range</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(Bypass_Sales_Price_Validation__c), !ISNEW(), ISCHANGED(UnitPrice),
    OR(
        AND(
            Product2.Name = &apos;DM Solution&apos;,
            UnitPrice &lt; 89.00
        ),
        AND(
            Product2.Name = &apos;DM Solution&apos;,
            UnitPrice &gt; 99.00
        ),
        AND(
            Product2.Name = &apos;CV Solution&apos;,
            UnitPrice &lt; 51.00
        ),
        AND(
            Product2.Name = &apos;CV Solution&apos;,
            UnitPrice &gt; 57.00
        ),
        AND(
            Product2.Name = &apos;DP Solution&apos;,
            UnitPrice &lt; 57.00
        ),
        AND(
            Product2.Name = &apos;DP Solution&apos;,
            UnitPrice &gt; 77.00
        )
    )
)</errorConditionFormula>
        <errorDisplayField>UnitPrice</errorDisplayField>
        <errorMessage>You&apos;re attempting to update the Sales Price outside of the defined range for this program.</errorMessage>
    </validationRules>
