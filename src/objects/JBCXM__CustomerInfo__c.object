        <fullName>JBCXM__LifeCycleInYear__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF(JBCXM__OriginalContractDate__c&lt;TODAY(),ABS(FLOOR((TODAY() - JBCXM__OriginalContractDate__c)/365)),0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Life cycle in year</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>JBCXM__LifeCycleWeeksPerYear__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF(JBCXM__OriginalContractDate__c&lt;TODAY(),ABS(CEILING(((TODAY() - JBCXM__OriginalContractDate__c) - JBCXM__LifeCycleInYear__c*365)/7)),0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Life cycle weeks per year</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>JBCXM__LifecycleInWeeks__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF(JBCXM__OriginalContractDate__c&lt;TODAY(),ABS(CEILING((JBCXM__OriginalContractDate__c - TODAY())/7)) ,0)</formula>
