                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Consultant introduced us, and was involved in selling</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>myOpportunity.Benefit_Consultant_Involved_in_Deal__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Consultant did not introduce us, but was involved in selling</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Relationship_InfluencerRelationshipType</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Relationship_LeadSourceRelationshipType</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>InfluencerRelationshipAccount_Text</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue></stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>LeadSourceRelationshipAccount_Text</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue></stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>InfluencerRelationshipAccount_Text</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>LeadSourceRelationshipAccount_Text</leftValueReference>
                <operator>IsNull</operator>
