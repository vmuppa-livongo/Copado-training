    </choices>
    <decisions>
        <name>Validate_Account_Contacts</name>
        <label>Validate Account Contacts</label>
        <locationX>644</locationX>
        <locationY>518</locationY>
        <defaultConnector>
            <targetReference>NoAccountContactsScreen</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Account Contacts Null</defaultConnectorLabel>
        <rules>
            <name>Account_Contacts_Not_Null</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Related_Account_Contacts</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>AccountContactSelectionScreen</targetReference>
            </connector>
            <label>Account Contacts Not Null</label>
        </rules>
    </decisions>
    <decisions>
        <name>Validate_Contact_Email_Address</name>
        <label>Validate Contact Email Address</label>
        <locationX>446</locationX>
        <locationY>758</locationY>
        <defaultConnector>
            <targetReference>Assign_Selected_Contact_Email</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Contact Email Exists</defaultConnectorLabel>
        <rules>
            <name>Contact_Email_is_Null</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>ContactEmail</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Email_Error_Message_Screen</targetReference>
            </connector>
            <label>Contact Email is Null</label>
        </rules>
    </decisions>
    <decisions>
        <name>Validate_current_users_profile</name>
        <label>Validate current users profile</label>
        <locationX>347</locationX>
        <locationY>158</locationY>
        <defaultConnector>
            <targetReference>Get_Related_Account_Record</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Current user allowed to send survey</defaultConnectorLabel>
        <rules>
            <name>Current_user_not_allowed_to_send_survey</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>fCurrent_User_Profile</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Client Implementation</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>fCurrent_User_Profile</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Client Success</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>fCurrent_User_Profile</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Enrollment Marketing</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>fCurrent_User_Profile</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>System Administrator</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Users_Not_Allowed_to_Send_Screen</targetReference>
            </connector>
            <label>Current user not allowed to send survey</label>
        </rules>
    </decisions>
    <dynamicChoiceSets>
        <name>AccountContactRecordChoiceSet</name>
        <dataType>String</dataType>
        <displayField>Contact_Name__c</displayField>
        <filterLogic>and</filterLogic>
        <filters>
            <field>AccountId</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>recordId</elementReference>
            </value>
        </filters>
        <filters>
            <field>Send_Enrollment_Survey__c</field>
            <operator>EqualTo</operator>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </filters>
        <limit>10</limit>
        <object>AccountContactRelation</object>
        <outputAssignments>
            <assignToReference>ContactEmail</assignToReference>
            <field>Contact_Email__c</field>
        </outputAssignments>
        <valueField>Contact_Name__c</valueField>
    </dynamicChoiceSets>
    <dynamicChoiceSets>
        <name>pEnrollement_Communications_Survey_Type</name>
        <dataType>Picklist</dataType>
        <displayField xsi:nil="true"/>
        <object xsi:nil="true"/>
        <picklistField>Enrollment_Communications_Survey_Type__c</picklistField>
        <picklistObject>Enrollment_Communications_Survey__c</picklistObject>
    </dynamicChoiceSets>
    <formulas>
        <name>fCurrent_User_Profile</name>
        <dataType>String</dataType>
        <expression>{!$Profile.Name}</expression>
    </formulas>
    <formulas>
        <name>fEnrollment_Communications_Survey_Community_URL</name>
        <dataType>String</dataType>
        <expression>{!$Label.Enrollment_Communications_Survey_Community_URL}</expression>
    </formulas>
    <formulas>
        <name>fEnrollment_Marketing_Lead_Id</name>
        <dataType>String</dataType>
        <expression>IF( ISBLANK({!Get_Related_Account_Record.EML__c}), &apos;&apos;, {!Get_Related_Account_Record.EML__r.Id})</expression>
    </formulas>
    <formulas>
        <name>fOrg_Base_Url</name>
        <dataType>String</dataType>
        <expression>LEFT({!$Api.Partner_Server_URL_340}, FIND( &apos;/services&apos;, {!$Api.Partner_Server_URL_340}))</expression>
    </formulas>
